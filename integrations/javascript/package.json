{
  "name": "@aaib/javascript",
  "version": "1.0.0",
  "dependencies": {
    "@types/node": "^16.18.34",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "npm run compile",
    "serve": "npx serve -s ./dist -l 6740",
